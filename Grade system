m = int(input())
if m in range(0,101):
    if m==100:
        print("S")
    elif m>=90 and m<100:
        print("A")
    elif m>=80 and m<90:
        print("B")
    elif m>=70 and m<80:
        print("C")
    elif m>=60 and m<70:
        print("D")
    elif m>=50 and m<60:
        print("E")
    else:
        print("F")
else:
    print("invalid input")
